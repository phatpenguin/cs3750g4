<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
	xmlns:ic="http://schemas.microsoft.com/expression/2010/interactions"
	xmlns:local="clr-namespace:BBQRMSSolution"
	mc:Ignorable="d"
	x:Class="BBQRMSSolution.Views.ChooseReport"
	d:DesignWidth="800" d:DesignHeight="480">
	<UserControl.Resources>
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" >
		<Grid.RowDefinitions>
			<RowDefinition Height="0.079*"/>
			<RowDefinition Height="0.921*"/>
		</Grid.RowDefinitions>
		<TextBlock TextWrapping="Wrap" Text="Select Report" Style="{DynamicResource PageHeadingStyle}"/>
	</Grid>
</UserControl>