<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ViewModels="clr-namespace:BBQRMSSolution.ViewModels"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Class="BBQRMSSolution.Views.MainWindow"
	Title="BBQ RMS"
	Width="1366"
	Height="768"
	WindowStyle="SingleBorderWindow"
	WindowState="Maximized"
	ResizeMode="CanResize"
	MinHeight="738"
	MinWidth="1024"
	mc:Ignorable="d"
	d:DesignHeight="500" 
	d:DesignWidth="857"
	d:DataContext="{d:DesignInstance ViewModels:MainWindowViewModel, IsDesignTimeCreatable=True}"
	>
	<ContentPresenter Content="{Binding Path=FullScreenContent}"/>
</Window>